{
  "name": "sourov-dash-portfolio",
  "version": "1.0.0",
  "private": true,
  "homepage": ".",
  "dependencies": {
    "@emailjs/browser": "^4.4.1",
    "framer-motion": "^12.23.27",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-icons": "^5.5.0",
    "react-scripts": "^5.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "build:optimize": "echo 'Build completed successfully'",
    "test": "react-scripts test --watchAll=false",
    "eject": "react-scripts eject",
    "analyze": "npm run build && npx serve -s build",
    "clean": "rm -rf build node_modules/.cache",
    "vercel-build": "react-scripts build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "no-console": "warn",
      "no-unused-vars": "warn"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
